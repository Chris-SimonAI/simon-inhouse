{
  "name": "meet-simon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "npm run start:prod",
    "start:prod": "npm run db:migrate && npm run db:seed && next start",
    "start:only": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint",
    "test": "vitest",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts --verbose",
    "db:seed": "node --experimental-strip-types scripts/seed.ts",
    "db:better-auth": "npx @better-auth/cli@latest generate --output=./src/db/schemas/auth.ts"
  },
  "dependencies": {
